<% layout('layout/boilerplate') %>
    <form action="/tcas" method="POST">
        <div>
            <label for="name">First Name</label>
            <input type="text" id="firstName", name="tca[firstName]">
        </div>
        <div>
            <label for="name">Last Name</label>
            <input type="text" id="lastName", name="tca[lastName]">
        </div>
        <div>
            <label for="name">Address</label>
            <input type="text" id="address", name="tca[address]">
        </div>
        <div>
            <label for="description">Description</label>
            <input type="text" id="description", name="tca[description]">
        </div>
        <button> Save </button>
    </form>
    <a href="/tcas">All TCAs</a>